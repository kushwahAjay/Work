package Scripts;

public class ArraysPrint {
	
	public static void main(String[] args) {
		 int a1[]={11,44,66,33};
		
		for(int i:a1)
		{
			System.out.println(i);
		}
		sort(a1);
		for(int i=0;i<a1.length;i++)
		{
			System.out.print(a1[i]);
		
		}
		 
	} 
	static void sort(int a1[])
	{
		 for(int i=0; i<a1.length;i++)
		 {
			 for(int j=1+1; j<=a1.length-1;j++)
			 {
				 if(a1[i]>a1[j])
				 {
					 int temp=0;
					 temp=a1[i];
					 a1[i]=a1[j];
					 a1[j]=temp;
				 }
				
			 }
			
		 }
		 
		
		 
		
	}

	}
